{
    "name": "vita-plus",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install"
    },
    "devDependencies": {
        "cz-conventional-changelog": "^3.3.0",
        "eslint-config-custom": "*",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "npm@8.19.1",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@types/axios": "^0.14.0",
        "axios": "^1.1.3"
    }
}
